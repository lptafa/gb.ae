use "@/cart.ae"


struct Bus {
    cart: &Cartridge
}

def Bus::new(cart: &Cartridge) -> Bus {
    let bus = calloc(1, sizeof(Bus)) as &Bus
    *bus = Bus(cart)
    return bus
}

